{
  "name": "knerdie",
  "version": "1.0.0",
  "description": "Cats",
  "main": "dist/index.js",
  "types": "dist/types/src/index.d.ts",
  "author": "Paul Sastrasinh, Giannis Neokleous, Joanna Ricou",
  "scripts": {
    "package": "npm-run-all --parallel build assets types",
    "assets": "cpx 'assets/**/*.{png,svg,jpg}' 'dist/assets'",
    "types": "tsc --declaration -p src",
    "build": "npm-run-all --parallel build:*",
    "build:src": "rollup -c rollup.config.prod.js",
    "build:lib": "rollup -c rollup.config.lib.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:src": "rollup --watch -c rollup.config.dev.js"
  },
  "devDependencies": {
    "@types/chai": "^3.5.1",
    "@types/loglevel": "^1.4.29",
    "@types/webfontloader": "^1.6.28",
    "cpx": "^1.5.0",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^2.10.0",
    "postcss-nested": "^1.0.1",
    "postcss-simple-vars": "^3.1.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-image": "^1.0.2",
    "rollup-plugin-livereload": "^0.4.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-builtins": "^2.1.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.4.1",
    "rollup-plugin-serve": "^0.3.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.2",
    "rollup-watch": "^3.2.2",
    "source-map-support": "^0.4.14",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2"
  },
  "dependencies": {
    "@types/pubsub-js": "^1.5.17",
    "konva": "^1.6.0",
    "loglevel": "^1.4.1",
    "pubsub-js": "^1.5.6",
    "webfontloader": "^1.6.28"
  }
}
